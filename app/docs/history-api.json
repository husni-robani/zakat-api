{
    "openapi": "3.0.3",
    "info": {
        "title": "history api specification",
        "description": "history api specification",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https"
        }
    ],
    "paths": {
        "/api/history/transactions": {
            "get": {
                "description": "Get transaction history",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "required": false,
                        "description": "search history by invoice_number, donor name, house_number, no_kk",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "good_type_id",
                        "in": "query",
                        "required": false,
                        "description": "filter by good type, value is id of good type",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "donation_type_id",
                        "in": "query",
                        "required": false,
                        "description": "filter by donation type, value is id of donation type",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "wallet_type_id",
                        "in": "query",
                        "required": false,
                        "description": "filter by wallet, value is id of wallet type",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "donor_type",
                        "in": "query",
                        "required": false,
                        "description": "filter by donor_type, value is resident or guest",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "success to get transaction history",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data" : {
                                            "properties": {
                                                "donation_type": {
                                                    "type": "string"
                                                },
                                                "donor_type": {
                                                    "type": "string"
                                                },
                                                "no_kk": {
                                                    "type": "string"
                                                },
                                                "donor_name": {
                                                    "type": "string"
                                                },
                                                "wallet": {
                                                    "type": "string"
                                                },
                                                "good_type": {
                                                    "type": "string"
                                                },
                                                "amount": {
                                                    "type": "integer"
                                                },
                                                "completed_status": {
                                                    "type": "boolean"
                                                },
                                                "created": {
                                                    "type": "string",
                                                    "pattern": "date"
                                                },
                                                "updated": {
                                                    "type": "string",
                                                    "pattern": "date"
                                                }
                                            }
                                        },
                                        "links": {
                                            "type": "object",
                                            "properties": {
                                                "first": {
                                                    "type": "string",
                                                    "format": "link"
                                                },
                                                "last": {
                                                    "type": "string",
                                                    "format": "link"
                                                },
                                                "prev": {
                                                    "type": "string",
                                                    "format": "link"
                                                },
                                                "next": {
                                                    "type": "string",
                                                    "format": "link"
                                                }
                                            }
                                        },
                                        "meta": {
                                            "type": "object"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "success to get transaction history without paginated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "donation_type": {
                                            "type": "string"
                                        },
                                        "donor_type": {
                                            "type": "string"
                                        },
                                        "no_kk": {
                                            "type": "string"
                                        },
                                        "donor_name": {
                                            "type": "string"
                                        },
                                        "wallet": {
                                            "type": "string"
                                        },
                                        "good_type": {
                                            "type": "string"
                                        },
                                        "amount": {
                                            "type": "integer"
                                        },
                                        "completed_status": {
                                            "type": "boolean"
                                        },
                                        "created": {
                                            "type": "string",
                                            "pattern": "date"
                                        },
                                        "updated": {
                                            "type": "string",
                                            "pattern": "date"
                                        }
                                    }
                                }
                            }
                        }
                    }

                }
            }
        }
    }
}
